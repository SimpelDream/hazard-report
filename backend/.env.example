# 服务器配置
PORT=3000
NODE_ENV=development

# 数据库配置
DATABASE_URL="postgresql://user:121@localhost:5432/hazard_report?schema=public"

# CORS配置
ALLOWED_ORIGINS=http://localhost:3000

# 安全配置
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=24h

# 日志配置
LOG_LEVEL=info
LOG_FILE_PATH=logs/app.log
LOG_MAX_SIZE=10m
LOG_MAX_FILES=14d

# 缓存配置
CACHE_TTL=3600 